{
    "name": "react-router-typesafe-routes",
    "version": "0.3.0",
    "description": "Comprehensive type-safe routes for react-router.",
    "keywords": [
        "typesafe",
        "type-safe",
        "react-router",
        "react"
    ],
    "repository": "github:fenok/react-router-typesafe-routes",
    "license": "MIT",
    "author": "Leonid Fenko <fenok2112@gmail.com>",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "sideEffects": false,
    "files": [
        "dist/"
    ],
    "scripts": {
        "format": "prettier --write --ignore-unknown .",
        "build": "rimraf dist && tsc --project tsconfig.build.json",
        "format:check": "prettier --check .",
        "lint": "eslint .",
        "test": "jest",
        "prepack": "yarn build",
        "prepublish": "yarn format:check && yarn lint && yarn test"
    },
    "devDependencies": {
        "@types/jest": "^26.0.23",
        "@types/react-router": "^5.1.15",
        "@typescript-eslint/eslint-plugin": "^4.27.0",
        "@typescript-eslint/parser": "^4.27.0",
        "conditional-type-checks": "^1.0.5",
        "eslint": "^7.29.0",
        "eslint-config-prettier": "^8.3.0",
        "jest": "^27.0.4",
        "prettier": "^2.3.1",
        "react": "^17.0.2",
        "react-router": "^5.2.0",
        "rimraf": "^3.0.2",
        "ts-jest": "^27.0.3",
        "typescript": "^4.3.4"
    },
    "peerDependencies": {
        "react-router": "^5.2.0"
    },
    "dependencies": {
        "@types/history": "*",
        "path-to-regexp": "^1.7.0",
        "query-string": "^7.0.0"
    }
}
