{
    "name": "react-router-typesafe-routes",
    "version": "0.5.1",
    "description": "Comprehensive type-safe routes for react-router v6 with first-class support for nested routes and param validation.",
    "keywords": [
        "typesafe",
        "type-safe",
        "validation",
        "react-router",
        "react-router6",
        "react"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/fenok/react-router-typesafe-routes.git"
    },
    "license": "MIT",
    "author": "Leonid Fenko <fenok2112@gmail.com>",
    "type": "module",
    "main": "common/index.js",
    "module": "common/index.js",
    "types": "common/index.d.ts",
    "exports": {
        ".": "./common/index.js",
        "./dom": "./dom/index.js",
        "./native": "./native/index.js",
        "./package.json": "./package.json"
    },
    "sideEffects": false,
    "files": [
        "common/",
        "dom/",
        "native/"
    ],
    "engines": {
        "node": ">=14"
    },
    "scripts": {
        "format": "prettier --write --ignore-unknown .",
        "build": "rimraf common dom native && tsc --project tsconfig.build.json --outDir .",
        "format:check": "prettier --check .",
        "lint": "eslint --max-warnings=0 --report-unused-disable-directives .",
        "test": "jest",
        "prepack": "yarn build",
        "prepublish": "yarn format:check && yarn lint && yarn test"
    },
    "devDependencies": {
        "@types/jest": "^27.5.1",
        "@types/react": "^18.0.9",
        "@typescript-eslint/eslint-plugin": "^5.25.0",
        "@typescript-eslint/parser": "^5.25.0",
        "conditional-type-checks": "^1.0.5",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "jest": "^28.1.0",
        "prettier": "^2.6.2",
        "react": ">=16.8",
        "react-router-dom": "^6.3.0",
        "react-router-native": "^6.3.0",
        "rimraf": "^3.0.2",
        "ts-jest": "^28.0.2",
        "typescript": "^4.6.4"
    },
    "peerDependencies": {
        "react": ">=16.8",
        "react-router-dom": "^6.3.0",
        "react-router-native": "^6.3.0"
    },
    "peerDependenciesMeta": {
        "react-router-dom": {
            "optional": true
        },
        "react-router-native": {
            "optional": true
        }
    },
    "packageManager": "yarn@3.2.1"
}
