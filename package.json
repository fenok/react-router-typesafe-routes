{
    "name": "react-router-typesafe-routes",
    "version": "0.3.2",
    "description": "Comprehensive type-safe routes for react-router.",
    "keywords": [
        "typesafe",
        "type-safe",
        "react-router",
        "react"
    ],
    "repository": "github:fenok/react-router-typesafe-routes",
    "license": "MIT",
    "author": "Leonid Fenko <fenok2112@gmail.com>",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "sideEffects": false,
    "files": [
        "dist/"
    ],
    "engines": {
        "node": "^16.14.2"
    },
    "scripts": {
        "format": "prettier --write --ignore-unknown .",
        "build": "rimraf dist && tsc --project tsconfig.build.json",
        "format:check": "prettier --check .",
        "lint": "eslint .",
        "test": "jest",
        "prepack": "yarn build",
        "prepublish": "yarn format:check && yarn lint && yarn test"
    },
    "devDependencies": {
        "@types/jest": "^27.5.1",
        "@types/react-router": "^5.1.18",
        "@typescript-eslint/eslint-plugin": "^5.25.0",
        "@typescript-eslint/parser": "^5.25.0",
        "conditional-type-checks": "^1.0.5",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.5.0",
        "jest": "^28.1.0",
        "prettier": "^2.6.2",
        "react": "^18.1.0",
        "react-router": "^6.3.0",
        "rimraf": "^3.0.2",
        "ts-jest": "^28.0.2",
        "typescript": "^4.6.4"
    },
    "peerDependencies": {
        "react-router": "^6.3.0"
    },
    "packageManager": "yarn@3.2.1"
